{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/video_trailer/(.*)",
      "dest": "/video_trailer/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/video_trailer/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp4"
        }
      ]
    }
  ]
}
